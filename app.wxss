/**app.wxss**/
page{
  width: 100%;
  height: 100%;
}
.container{
  background: #F5F5F5;
  min-height: 100%;
  border-top: 2rpx solid #F5F5F5;
}
swiper{
  -webkit-overflow-scrolling:touch;
}
swiper::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.fontweight{
  font-weight: bold !important;
}
button{
  background: none;
}
button::after{
  border: none;
}
.btn-hover{
  background: #FFFFFF;
}
rich-text .rich-img{
  max-width: 100%;
  height: auto;
}