<view class='container'>
  <view style='background:#FFFFFF;margin-bottom:10rpx;'>
    <view class='conversuccess-icon'>
      <image src='/images/conversuccess@2x.png'></image>
      <text>兑换成功</text>
    </view>
    <view class='haveaddress' bindtap='toDetails'>
      <view class='address-font'>
        <view class='textoverflow'>
          <text>收件人：</text>
          <text>{{address_name}}，</text>
          <text>{{address_telephone}}</text>
        </view>
        <view class='textoverflow'>{{address_address}}</view>
      </view>
      <image src='/images/arrow@2x.png' class='address-icon'></image>
    </view>
    <view class='goonmove fontweight'>
      <view class='goonmove-btn' bindtap='goOn'>继续逛逛</view>
    </view>
  </view>
  <view style='background:#FFFFFF;'>
     <view class='conversuccess-header'>
      <image src='{{image}}' style='width:146rpx;height:146rpx;'></image>
      <view class='conversuccess-header-text'>
        <text class='header-text1 textoverflow'>{{title}}</text>
        <text class='header-text2'>x1</text>
        <view class='header-price'>
          <image src='/images/woolbigicon@2x.png' style='width:24rpx;height:24rpx;margin-top:-1rpx;'></image>
          <text class='header-text3 fontweight'>{{yangmao}}</text>
        </view>
      </view>
     </view>
     <view class='conversuccess-li'>
        <view class='conversuccess-li-font'>运费</view>
        <view class='conversuccess-li-font' style='color:#999999;'>￥ 0</view>
     </view>
     <view class='conversuccess-li'>
        <view class='conversuccess-li-font'>羊毛总额</view>
        <view class='conversuccess-li-font' style='color:#999999;'>{{yangmao}}</view>
     </view>
     <view class='conversuccess-li'>
        <view class='conversuccess-li-font'>剩余羊毛</view>
        <view class='conversuccess-li-font' style='color:#999999;'>{{shengyu_yangmao}}</view>
     </view>
     <view class='conversuccess-li'>
        <view class='conversuccess-li-font'>订单编号</view>
        <view class='conversuccess-li-font' style='color:#999999;'>{{orderno}}</view>
     </view>
     <view class='conversuccess-li'>
        <view class='conversuccess-li-font'>兑换时间</view>
        <view class='conversuccess-li-font' style='color:#999999;'>{{dateline}}</view>
     </view>
     <view style='width:100%;height:60rpx;margin-top:2rpx;'></view>
  </view>
</view>