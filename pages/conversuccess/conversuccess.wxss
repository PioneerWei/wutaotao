.conversuccess-icon{
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 100rpx;
  padding-bottom: 60rpx;
}
.conversuccess-icon image{
  width: 112rpx;
  height: 112rpx;
}
.conversuccess-icon text{
  display: inline-block;
  margin-top: 20rpx;
  font-size: 24rpx;
  line-height: 33rpx;
  color: #999999;
}
.haveaddress{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx;
  border-bottom: 2rpx solid #F7F7F7;
  border-top: 2rpx solid #F7F7F7;
}
.address-font{
  font-size: 28rpx;
  line-height: 40rpx;
}
.address-icon{
  width: 12rpx;
  height: 23rpx;
}
.goonmove{
  padding: 40rpx 0;
}
.goonmove-btn{
  margin: auto;
  width: 238rpx;
  line-height: 66rpx;
  background: linear-gradient(to right,rgba(255,191,54,0.88), #FF9844);
  color: #FFFFFF;
  font-size: 26rpx;
  text-align: center;
  border-radius: 33rpx;
}
.conversuccess-header{
  padding: 20rpx;
  display: flex;
  background: #FFFFFF;
  border-bottom: 2rpx solid #F5F5F5;
}
.conversuccess-header-text{
  margin-left: 20rpx;
}
.header-text1{
  font-size: 28rpx;
  line-height: 40rpx;
}
.header-text2{
  display: block;
  margin-top: 10rpx;
  margin-bottom: 20rpx;
  font-size: 24rpx;
  line-height: 33rpx;
}
.header-price{
  display: flex;
  align-items: center
}
.header-text3{
  display: inline-block;
  margin-left: 10rpx;
  font-size: 24rpx;
  color: #FFC600;
}
.conversuccess-li{
  display: flex;
  justify-content: space-between;
  margin-left: 30rpx;
  width: 720rpx;
  padding: 39rpx 30rpx 30rpx 0;
  box-sizing: border-box;
  margin-top: 2rpx;
  background: #FFFFFF;
  border-bottom: 2rpx solid #F5F5F5;
}
.conversuccess-li-font{
  font-size: 28rpx;
  line-height: 40rpx;
}
.textoverflow{
  display: block;
  width: 530rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}